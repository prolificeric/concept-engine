{
  "name": "concept-engine",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prebuild": "npm install -g pnpm"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/node": "^17.0.35",
    "esbuild": "^0.14.39",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "turbo": "^1.2.12",
    "typescript": "^4.6.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": "14.x"
  },
  "packageManager": "pnpm@6.31.0"
}
